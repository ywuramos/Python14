import math
an = float(input('Digite um 창ngulo: '))
seno = math.sin(math.radians(an))
cos = math.cos(math.radians(an))
tan = math.tan(math.radians(an))
print('O 창ngulo de {:.0f} tem o SENO de {:.2f}'.format(an, seno))
print('O 창ngulo de {:.0f} tem o COSSENO de {:.2f}'.format(an, cos))
print('O 창ngulo de {:.0f} tem a TANGENTE de {:.2f}'.format(an, tan))
